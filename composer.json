{
    "name": "arnedesmedt/event-engine-symfony-bundle",
    "type": "symfony-bundle",
    "description": "A library that creates a bridge between Event Engine and Symfony.",
    "homepage": "https://github.com/arnedesmedt/event-engine-symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Arne De Smedt",
            "email": "arnedesmdt@gmail.com",
            "homepage": "https://github.com/arnedesmedt"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "event-engine/php-engine": "^0",
        "event-engine/php-json-schema": "^2",
        "event-engine/php-postgres-document-store": "^0",
        "event-engine/prooph-v7-event-store": "^0",
        "prooph/pdo-event-store": "^1",
        "opis/json-schema": "^2",
        "arnedesmedt/value-objects": "^0",
        "arnedesmedt/json-immutable-objects": "^0",
        "symfony/cache": "^5.1",
        "symfony/http-kernel": "^5.1",
        "arnedesmedt/php-util": "^0",
        "symfony/validator": "^5.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1",
        "squizlabs/php_codesniffer": "^3",
        "doctrine/coding-standard": "^9",
        "phpro/grumphp": "^1"
    },
    "autoload": {
        "psr-4": {
            "ADS\\Bundle\\EventEngineBundle\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
